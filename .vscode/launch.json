  "version": "0.2.0",
  "configurations": [ 
    {
      "name": "run web release",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["-d", "chrome", "--web-browser-flag=--disable-web-security"],
    },
    {
      "name": "run web debug",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["-d", "chrome", "--web-browser-flag=--disable-web-security"],
    },
    {
      "name": "run emulator android debug",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": ["-d", "emulator-5554"],
    }
  ]
}